{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/Config",
    "definitions": {
        "Config": {
            "type": "object",
            "additionalProperties": true,
            "properties": {
                "type": {
                    "type": "string"
                },
                "setup_class": {
                    "type": "string",
                    "pattern": "^[A-Z][a-z|A-Z|0-9]+PullerSetup$"
                },
                "module_properties": {
                    "$ref": "#/definitions/ModuleProperties"
                }
            },
            "required": [
                "setup_class",
                "type",
                "module_properties"
            ],
            "title": "Internal Config"
        },
        "ModuleProperties": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "max_number_of_messages_per_page": {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 10000
                },
                "base_url": {
                    "type": "string"
                },
                "base_tag": {
                  "type": "string"
                },
                "parameter_2": {
                  "type": "string"
                }
            },
            "required": [
                "max_number_of_messages_per_page",
                "base_url",
                "base_tag",
                "parameter_2"
            ],
            "title": "Internal Config"
        }
    }
}