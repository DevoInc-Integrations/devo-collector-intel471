{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/Config",
    "definitions": {
        "Config": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "override_tag": {
                    "type": "string",
                    "pattern": "^(([a-z]|_|[0-9])+[.]){3,7}([a-z]|_|[0-9])+$"
                },
                "override_limit_data_per_request": {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 100
                },
                "historic_date_utc": {
                    "type": "string",
                    "format": "%Y-%m-%d %H:%M:%S"
                },
                "flatten_entities": {
                    "type": "boolean"
                },
                "request_period_in_seconds": {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 604800
                },
                "obfuscation_data":{
                    "type": "array"
                }
            },
            "title": "Config"
        }
    }
}